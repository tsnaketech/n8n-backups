        {
            "parameters": {
                "jsCode": "// Trouver la table par nom\nconst tables = $input.first().json.list;\nconst config = $('Configuration').first().json.list;\n\nconst targetTable = tables.find(table => table.table_name === config.TABLE_NAME);\n\nif (!targetTable) {\n  throw new Error(`Table '${config.TABLE_NAME}' introuvable dans la base`);\n}\n\nreturn {\n  json: {\n    table_id: targetTable.id,\n    table_name: targetTable.table_name,\n    table_title: targetTable.title,\n    found: true\n  }\n};"
            },
